{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "packages": ["packages/*", "helpers/*"],
  "command": {
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish [skip-ci]",
      "ignoreChanges": ["**/*.md", "**/test/*.test.ts"]
    }
  }
}
